	keywords = {Animals, Circadian Rhythm, Histones, Liver, Metabolic Networks and Pathways, Mice, Proteins, SIRT1, Transcriptome, Caloric Restriction, Acetyl Coenzyme A, Acetylation, Aging, Circadian Clock, Liver Metabolism, NAD, Reprogramming, Sirtuin 1, Stem Cells},
	pages = {664--677.e11}
}

@article{kolbe_scn_2016,
